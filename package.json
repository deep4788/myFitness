{
  "name": "myfitness",
  "productName": "MyFitness",
  "version": "1.0.0",
  "description": "A personal fitness tracker built using Electron (Node.js, JavaScript, HTML, CSS).",
  "author": "Deep Aggarwal <deep.uiuc@gmail.com>",
  "main": "app",
  "scripts": {
    "package-osx": "electron-packager . --out=dist --overwrite --platform=darwin --arch=x64 --icon=resources/app-icons/stable/myfitness.icns && cp -r dist/MyFitness-darwin-x64/MyFitness.app /Applications",
    "start": "electron app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deep4788/myFitness"
  },
  "engines": {
    "node": "4.6.0",
    "npm": "2.15.9"
  },
  "keywords": [
    "electron",
    "node.js",
    "javascript",
    "fitness",
    "health",
    "tracker",
    "handlebars"
  ],
  "dependencies": {
    "electron": "^1.6.1",
    "electron-packager": "8.3.0",
    "fs.extra": "1.3.2",
    "handlebars": "4.0.6",
    "jsonfile": "2.4.0"
  },
  "license": "MIT"
}
